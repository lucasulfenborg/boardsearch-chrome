{
    "manifest_version": 3,
    "name": "4chan Board Search",
    "version": "1.0",
    "description": "Search 4chan boards directly from your browser.",
    "permissions": [
      "activeTab",
      "storage"
    ],
    
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }

  